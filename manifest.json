{
  "manifest_version": 2,

  "name": "Full Width Articles",
  "description": "Makes selected element full width",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["mystyle.css"],
      "js": ["myscript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}